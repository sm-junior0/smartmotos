{
  "expo": {
    "name": "smart-motos-app",
    "slug": "smart-motos-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiYXBjZ3JvdXAiLCJhIjoiY21jZTFlOGQ5MGhzdjJtc2FzNm1uM3J5YSJ9.zN2GPvPyH4xT_PtlcRk7NA"
        }
      ],
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "13cea3bb-2382-4cd1-8f84-147ce42dd019"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.nihonor.smartmotosapp"
    }
  }
}
